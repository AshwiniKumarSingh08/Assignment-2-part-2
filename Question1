class Pair<K,V>{
	private K key;
	private V value;
	public Pair(K key, V value) {
		super();
		this.key = key;
		this.value = value;
	}
	public K getKey() {
		return key;
	}
	public void setKey(K key) {
		this.key = key;
	}
	public V getValue() {
		return value;
	}
	public void setValue(V value) {
		this.value = value;
	}
	@Override
	public String toString() {
		return "Pair [key=" + key + ", value=" + value + "]";
	}
}
public class Main {

	public static void main(String[] args) {
		Pair<Integer,String> p1=new Pair<>(12,"ABC");
		Pair<String,Double> p2=new Pair<>("DEF",12.5);
		System.out.println(p1.toString());
		System.out.println(p2.toString());
	}

}
